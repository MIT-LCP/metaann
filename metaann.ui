            <property name="pack_type">end</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
    <action-widgets>
      <action-widget response="2">button2</action-widget>
      <action-widget response="1">button1</action-widget>
    </action-widgets>
